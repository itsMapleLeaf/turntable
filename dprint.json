{
  "typescript": {
    "semiColons": "asi",
    "lineWidth": 100,
    "jsxAttributes.preferHanging": true,
    "jsx.multiLineParens": "never"
  },
  "json": {
  },
  "markdown": {
  },
  "includes": ["**/*.{ts,tsx,js,jsx,cjs,mjs,json,md}"],
  "excludes": [
    "**/node_modules",
    "build",
    "public/build",
    "dist"
  ],
  "plugins": [
    "https://plugins.dprint.dev/typescript-0.85.0.wasm",
    "https://plugins.dprint.dev/json-0.17.4.wasm",
    "https://plugins.dprint.dev/markdown-0.15.3.wasm"
  ]
}
