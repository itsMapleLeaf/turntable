    "postcss": "^8.4.23",
    "pretty-ms": "^8.0.0",
    "solid-js": "^1.7.5",
    "zod": "^3.21.4"
  },
  "devDependencies": {
    "@itsmapleleaf/configs": "^3.0.1",
    "@types/eslint": "^8.37.0",
    "@types/node": "^20.1.7",
    "eslint": "^8.40.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.8.8",
    "tailwindcss": "^3.3.2",
    "typescript": "5.1.0-beta",
    "vite": "^4.3.8",
    "vite-plugin-solid": "^2.7.0"
  }
