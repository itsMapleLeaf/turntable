{
  "name": "turntable",
  "private": true,
  "engines": {
    "node": "18"
  },
  "packageManager": "pnpm@8.4.0",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "start": "vite preview",
    "lint": "run-p --continue-on-error --print-label lint:*",
